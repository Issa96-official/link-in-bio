  "version": "1.0.0",
  "type": "module",
  "homepage": "https://issa96-official.github.io/link-in-bio/",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:github": "cross-env VITE_GITHUB_PAGES=true tsc && vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "prepare-gh-pages": "node prepare-gh-pages.cjs",
    "fix-gh-pages": "node fix-github-pages.cjs",
    "predeploy": "npm run build:github && npm run fix-gh-pages",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.30.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
